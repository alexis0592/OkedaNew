            <ImageView
                android:id="@+id/imv4"
                android:layout_width="150dp"
                android:layout_height="150dp"
                android:layout_gravity="center_horizontal"
                android:layout_weight="4"
                android:contentDescription="6"
                android:onClick="listener3"
                android:src="@drawable/abc_ab_bottom_solid_light_holo" />

            <ImageView
                android:id="@+id/flechaDerecha4"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:contentDescription="3"
                android:onClick="mueveAIzquierda"
                android:src="@drawable/disclosure_icon" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

            <ImageView
                android:id="@+id/flechaIzquierda5"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:contentDescription="3"
