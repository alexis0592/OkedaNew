<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="co.gov.fna.okeda.presentacion.actividades.MostrarVivienda"
    tools:ignore="MergeRootFrame" >

    <ViewFlipper
        android:id="@+id/flipper2"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_weight="1"
        android:flipInterval="3000"
        android:inAnimation="@android:anim/slide_in_left"
        android:outAnimation="@android:anim/slide_out_right" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

            <ImageView
                android:id="@+id/flechaIzquierda1"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:onClick="MueveADerecha"
                android:src="@drawable/disclosure_icon2" />

            <ImageView
                android:id="@+id/imv1"
                android:layout_width="150dp"
                android:layout_height="150dp"
                android:layout_gravity="center_horizontal"
                android:layout_weight="3"
                android:contentDescription="2"
                android:onClick="listener1"
                android:src="@drawable/abc_ab_bottom_solid_light_holo" />

            <ImageView
                android:id="@+id/flechaDerecha1"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:onClick="mueveAIzquierda"
                android:src="@drawable/disclosure_icon" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

            <ImageView
                android:id="@+id/flechaIzquierda2"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:onClick="MueveADerecha"
                android:src="@drawable/disclosure_icon2" />

            <ImageView
                android:id="@+id/imv2"
                android:layout_width="150dp"
                android:layout_height="150dp"
                android:layout_gravity="center_horizontal"
                android:layout_weight="3"
                android:contentDescription="4"
                android:onClick="listener2"
                android:src="@drawable/abc_ab_bottom_solid_light_holo" />

            <ImageView
                android:id="@+id/flechaDerecha2"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:onClick="mueveAIzquierda"
                android:src="@drawable/disclosure_icon" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

            <ImageView
                android:id="@+id/flechaIzquierda3"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:contentDescription="3"
                android:onClick="MueveADerecha"
                android:src="@drawable/disclosure_icon2" />

            <ImageView
                android:id="@+id/imv3"
                android:layout_width="150dp"
                android:layout_height="150dp"
                android:layout_gravity="center_horizontal"
                android:layout_weight="3"
                android:contentDescription="5"
                android:onClick="listener3"
                android:src="@drawable/abc_ab_bottom_solid_light_holo" />

            <ImageView
                android:id="@+id/flechaDerecha3"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:contentDescription="3"
                android:onClick="mueveAIzquierda"
                android:src="@drawable/disclosure_icon" />
